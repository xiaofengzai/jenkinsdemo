<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.enshub.mapper.UserMapper">

    <select id="queryByName" parameterType="String" resultType="com.enshub.entity.User">
        SELECT * from user where `name`= #{name};
    </select>

</mapper>
